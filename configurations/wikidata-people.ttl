<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/wikidata/personalities/filmmaker> a browser:ExpansionQuery ;
  dct:title "Movies"@en ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/wikidata> ;
