    ?movie wdt:P57 ?node .
    BIND("director" AS ?directorClass)
  } UNION {
    ?movie wdt:P58 ?node .
    BIND("screenwriter" AS ?screenwriterClass)
  } UNION {
    ?movie wdt:P162 ?node .
    BIND("producer" AS ?producerClass)
  }

  SERVICE wikibase:label { bd:serviceParam wikibase:language "en,cs". }
  
}""" .

