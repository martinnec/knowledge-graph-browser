PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>
CONSTRUCT {  
  ?work a av:work ;
    rdfs:label ?workLabel ;
    browser:class "work" .
    
  ?work av:author ?node .
  av:author browser:class "author" .
    
} WHERE { 
  {
    ?work wdt:P50 ?node .
  } UNION {
    ?node wdt:P800 ?work .
  }

  SERVICE wikibase:label { bd:serviceParam wikibase:language "en,cs". }
  
}""" .

